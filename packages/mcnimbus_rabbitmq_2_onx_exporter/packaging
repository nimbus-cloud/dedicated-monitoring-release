#!/usr/bin/env bash

set -eux

# Copy common utils
mkdir -p ${BOSH_INSTALL_TARGET}/common
cp -a ${BOSH_COMPILE_TARGET}/common/* ${BOSH_INSTALL_TARGET}/common

# Extract mcnimbus_rabbitmq_2_onx_exporter package
mkdir -p ${BOSH_INSTALL_TARGET}/bin
tar xzvf ${BOSH_COMPILE_TARGET}/mcnimbus_rabbitmq_2_onx_exporter/mcnimbus_rabbitmq_2_onx_exporter-0.29.0.linux-amd64.tar.gz
cp -a ${BOSH_COMPILE_TARGET}/mcnimbus_rabbitmq_2_onx_exporter-0.29.0.linux-amd64/* ${BOSH_INSTALL_TARGET}/bin
