check process digitial_cap_redis_exporter
  with pidfile /var/vcap/sys/run/digitial_cap_redis_exporter/digitial_cap_redis_exporter.pid
  start program "/var/vcap/jobs/digitial_cap_redis_exporter/bin/digitial_cap_redis_exporter_ctl start"
  stop program "/var/vcap/jobs/digitial_cap_redis_exporter/bin/digitial_cap_redis_exporter_ctl stop"
  group vcap