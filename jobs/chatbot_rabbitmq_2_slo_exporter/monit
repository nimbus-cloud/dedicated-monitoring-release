check process chatbot_rabbitmq_2_slo_exporter
  with pidfile /var/vcap/sys/run/chatbot_rabbitmq_2_slo_exporter/chatbot_rabbitmq_2_slo_exporter.pid
  start program "/var/vcap/jobs/chatbot_rabbitmq_2_slo_exporter/bin/chatbot_rabbitmq_2_slo_exporter_ctl start"
  stop program "/var/vcap/jobs/chatbot_rabbitmq_2_slo_exporter/bin/chatbot_rabbitmq_2_slo_exporter_ctl stop"
  group vcap