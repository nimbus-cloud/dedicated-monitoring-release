check process chatbot_rabbitmq_2_hem_exporter
  with pidfile /var/vcap/sys/run/chatbot_rabbitmq_2_hem_exporter/chatbot_rabbitmq_2_hem_exporter.pid
  start program "/var/vcap/jobs/chatbot_rabbitmq_2_hem_exporter/bin/chatbot_rabbitmq_2_hem_exporter_ctl start"
  stop program "/var/vcap/jobs/chatbot_rabbitmq_2_hem_exporter/bin/chatbot_rabbitmq_2_hem_exporter_ctl stop"
  group vcap