check process chatbot_mongodb_exporter
  with pidfile /var/vcap/sys/run/chatbot_mongodb_exporter/chatbot_mongodb_exporter.pid
  start program "/var/vcap/jobs/chatbot_mongodb_exporter/bin/chatbot_mongodb_exporter_ctl start"
  stop program "/var/vcap/jobs/chatbot_mongodb_exporter/bin/chatbot_mongodb_exporter_ctl stop"
  group vcap
